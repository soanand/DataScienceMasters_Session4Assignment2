   "source": [
    "#import the required library, numpy \n",
    "import numpy as np\n",
    "\n",
    "#create a list of all the data\n",
    "data = [3, 21, 98, 203, 17, 9]\n",
    "\n",
    "#calculate the variance first \n",
    "varinace = np.var(data)\n",
    "\n",
    "#print the result \n",
    "print(\"Variance is : \",varinace)"
